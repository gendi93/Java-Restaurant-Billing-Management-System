<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="main" prefHeight="180.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SearchController">
   <children>
      <ComboBox fx:id="searchByComboBox" layoutX="25.0" layoutY="25.0" prefWidth="150.0" promptText="Search By" />
      <Button layoutX="50.0" layoutY="130.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button layoutX="100.0" layoutY="130.0" mnemonicParsing="false" onAction="#search" text="Search" />
   </children>
</AnchorPane>
